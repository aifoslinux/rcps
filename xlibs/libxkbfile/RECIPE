PKG=libxkbfile
VER=1.0.9
SRC=http://xorg.freedesktop.org/releases/individual/lib/$PKG-$VER.tar.bz2

build() {
    autoreconf -fiv
    ./configure --prefix=$prefix \
                --host=$CHOST \
                --sysconfdir=$cfgdir \
                --disable-static
    make
    make DESTDIR=$PKG_DIR install
}
