PKG=gsettings-desktop-schemas
VER=3.24.0
SRC=https://download.gnome.org/sources/$PKG/${VER:0:4}/$PKG-$VER.tar.xz

build() {
    ./configure --prefix=$prefix \
                --host=$CHOST \
                --sysconfdir=$cfgdir \
                --localstatedir=$vardir \
                --disable-schemas-compile
    make
    make DESTDIR=$PKG_DIR install
}
